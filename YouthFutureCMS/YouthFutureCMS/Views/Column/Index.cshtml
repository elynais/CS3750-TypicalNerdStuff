@model IEnumerable<YouthFutureCMS.Models.Column>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.columnHeader)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.columnDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.columnLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sectionNum)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.columnHeader)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.columnDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.columnLink)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sectionNum)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.columnId }) |
            @Html.ActionLink("Details", "Details", new { id=item.columnId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.columnId })
        </td>
    </tr>
}

</table>
